package{	import awaybuilder.WorldBuilder;	import awaybuilder.collada.ColladaLoader;		import flash.display.Sprite;	import flash.events.Event;	import flash.net.URLRequest;				public class Main extends Sprite	{		public function Main ( )		{			var loader : ColladaLoader = new ColladaLoader ( ) ;						loader.addEventListener ( Event.COMPLETE , this.loadComplete ) ;			loader.load ( new URLRequest ( "maya/world.dae" ) ) ;		}								protected function loadComplete ( event : Event ) : void		{			var loader : ColladaLoader = event.target as ColladaLoader ;			var world : WorldBuilder = new WorldBuilder ( ) ;						world.data = loader.collada ;			world.startCamera = "overviewCam" ;			world.addEventListener ( Event.COMPLETE , this.worldComplete ) ;			this.addChild ( world ) ;			world.x = this.stage.stageWidth * 0.5 ;			world.y = this.stage.stageHeight * 0.5 ;			world.build ( ) ;		}								protected function worldComplete ( event : Event ) : void		{			trace ( "worldComplete" ) ;		}	}}