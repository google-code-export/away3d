package away3d.exporters.data{	import flash.display.BitmapData;		public final class ImageData	{		public var image:BitmapData;		public var transparent:Boolean;		public var filename:String;		public var compress:int;				public function ImageData(image:BitmapData, transparent:Boolean = false, filename:String = "", compress:int = 100):void		{			 this.image = image;			 this.transparent = transparent;			 this.filename = filename;			 this.compress = compress;		}	}}